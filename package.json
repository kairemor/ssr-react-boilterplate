{
  "name": "react-ssr-boilerplate",
  "version": "1.1.0",
  "description": "Server-side Rendering React from Scratch!",
  "main": "index.js",
  "scripts": {
    "dev:client": "webpack-dev-server --open --config=config/webpack.config.js/client.dev.js",
    "start": "yarn build:server:dev && nodemon src/index.js",
    "start:prod": "yarn build:server:prod && node src/index.js",
    "build:server:dev": "webpack  --config config/webpack.config.js/server.dev.js",
    "build:server": "webpack --config config/webpack.config.js/server.prod.js",
    "build:client:dev": "webpack  --config config/webpack.config.js/client.dev.js",
    "build:client": "webpack --config config/webpack.config.js/client.prod.js",
    "lint:js": "eslint --ext .js,.jsx src/",
    "lint:css": "stylelint src/*"
  },
  "author": "KMTC",
  "license": "MIT",
  "dependencies": {
    "@babel/register": "^7.8.6",
    "chalk": "^4.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "i18next": "^19.5.4",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-express-middleware": "^2.0.0",
    "i18next-http-backend": "^1.0.16",
    "i18next-node-fs-backend": "^2.1.3",
    "ip": "^1.1.5",
    "nodemon": "^2.0.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.7.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "prettier-stylelint": "^0.4.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
